        for(key in grade){
            document.write("<li>key: "+key+" value :"+grade[key]+"</li>" );
            //배열에 안에 있는것을 가져올때는 for in문 을 사용 앞쪽은 키값 뒤쪽은 객체의 이름
        }
            for(key in grade)
        {
            console.log(grade[key]);
        }
